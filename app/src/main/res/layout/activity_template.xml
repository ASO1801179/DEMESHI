<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Template">
    <Button
        android:id="@+id/Back"
        android:layout_width="@dimen/BackBtnWidth"
        android:layout_height="@dimen/BackBtnHeight"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="@string/BackHint"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Address"
        android:layout_width="@dimen/TemplateTextWidth"
        android:layout_height="@dimen/TemplateTextHeight"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="56dp"
        android:gravity="left|center_vertical"
        android:text="@string/Company"
        android:textSize="@dimen/TemplateTextSize"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Template4" />

    <TextView
        android:id="@+id/Individual"
        android:layout_width="@dimen/TemplateTextWidth"
        android:layout_height="@dimen/TemplateTextHeight"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="80dp"
        android:gravity="left|center_vertical"
        android:text="@string/Individual"
        android:textSize="@dimen/TemplateTextSize"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/mixer"
        android:layout_width="@dimen/TemplateTextWidth"
        android:layout_height="@dimen/TemplateTextHeight"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:gravity="left|center_vertical"
        android:text="@string/mixer"
        android:textSize="@dimen/TemplateTextSize"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Template1" />

    <ImageView
        android:id="@+id/Template1"
        android:layout_width="@dimen/TemplateImgWidth"
        android:layout_height="@dimen/TemplateImgHeight"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toStartOf="@+id/Template2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Individual"
        app:srcCompat="@drawable/new1" />

    <ImageView
        android:id="@+id/Template7"
        android:layout_width="@dimen/TemplateImgWidth"
        android:layout_height="@dimen/TemplateImgHeight"
        android:layout_marginEnd="36dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="36dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toStartOf="@+id/Template8"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Address"
        app:srcCompat="@drawable/new7" />

    <ImageView
        android:id="@+id/Template2"
        android:layout_width="@dimen/TemplateImgWidth"
        android:layout_height="@dimen/TemplateImgHeight"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Individual"
        app:srcCompat="@drawable/new2" />

    <ImageView
        android:id="@+id/Template3"
        android:layout_width="@dimen/TemplateImgWidth"
        android:layout_height="@dimen/TemplateImgHeight"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/Template2"
        app:layout_constraintTop_toBottomOf="@+id/Individual"
        app:srcCompat="@drawable/new3" />

    <ImageView
        android:id="@+id/Template4"
        android:layout_width="@dimen/TemplateImgWidth"
        android:layout_height="@dimen/TemplateImgHeight"
        android:layout_marginEnd="36dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="36dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toStartOf="@+id/Template5"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mixer"
        app:srcCompat="@drawable/new4" />

    <ImageView
        android:id="@+id/Template5"
        android:layout_width="@dimen/TemplateImgWidth"
        android:layout_height="@dimen/TemplateImgHeight"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mixer"
        app:srcCompat="@drawable/new5" />

    <ImageView
        android:id="@+id/Template6"
        android:layout_width="@dimen/TemplateImgWidth"
        android:layout_height="@dimen/TemplateImgHeight"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/Template5"
        app:layout_constraintTop_toBottomOf="@+id/mixer"
        app:srcCompat="@drawable/new6" />

    <ImageView
        android:id="@+id/Template8"
        android:layout_width="@dimen/TemplateImgWidth"
        android:layout_height="@dimen/TemplateImgHeight"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Address"
        app:srcCompat="@drawable/new8" />

    <ImageView
        android:id="@+id/Template9"
        android:layout_width="@dimen/TemplateImgWidth"
        android:layout_height="@dimen/TemplateImgHeight"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/Template8"
        app:layout_constraintTop_toBottomOf="@+id/Address"
        app:srcCompat="@drawable/new9" />
</android.support.constraint.ConstraintLayout>